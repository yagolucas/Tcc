function macroBlocos = getBlocksTrans(imagem,crop) 
    [width, height] = size(imagem);
    copia = imagem;
    copia = imres
    for i = crop+1:crop:width
        for j = crop+1:crop:height
            copia(i-crop:i, j-crop:j) = dct2(copia(i-crop:i, j-crop:j));
            widthIndex = i
            heightIndex = j
        end
    end
    imshow(copia);
    macroBlocos = copia;
end